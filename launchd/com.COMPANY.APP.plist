<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

    <key>Label</key>
    <string>com.COMPANY.PROJECT</string>

    <key>Program</key>
    <string>/Applications/PROJECT.app/Contents/MacOS/Electron</string>

    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>

    <key>StandardOutPath</key>
    <string>/tmp/com.COMPANY.PROJECT.out.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.COMPANY.PROJECT.err.log</string>

    <key>ProcessType</key>
    <string>Interactive</string>

</dict>
</plist> 
